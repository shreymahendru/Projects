<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emergencyroommanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.emergencyroommanager.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.PatientListsActivity"
            android:label="@string/title_activity_patient_lists" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.PatientLookUpActivity"
            android:label="@string/title_activity_patient_look_up" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.FoundPatientActivity"
            android:label="@string/title_activity_found_pateint" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.AddPatientActivity"
            android:label="@string/title_activity_add_patient" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.VisitingPatient"
            android:label="@string/title_activity_visiting_patient" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.VisitingPatientActivity"
            android:label="@string/title_activity_visting_patients" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.GetVitalSignsActivity"
            android:label="@string/title_activity_get_vital_signs" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.PatientInfoActivity"
            android:label="@string/title_activity_patient_info" >
        </activity>
        <activity
            android:name="com.example.emergencyroommanager.DataSavedActivity"
            android:label="@string/title_activity_data_saved" >
        </activity>
    </application>

</manifest>
